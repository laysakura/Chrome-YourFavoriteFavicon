{
  "manifest_version": 2,
  "name": "your-favorite-favicon",
  "description": "You can choose your favorite favicon on each site.",
  "version": "0.1.0",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://example.com/*"],
      "js": ["your-favorite-favicon.js"],
      "run_at": "document_end"
    }
  ]
}
